# *****************************************
# FLAGS
# *****************************************
CFLAGS	= -O2

# *****************************************
# OBJEKTFILES
# *****************************************
OBJS = tcpip.o res.o

all: tcpip.mpn

# *****************************************
# COMPILER
# *****************************************
.c.o:
	pip-gcc -c $(CFLAGS) -o $@ $<

# *****************************************
# LINKER
# *****************************************
tcpip.mpn: $(OBJS)	
	pip-gcc -o $@ $(OBJS) -mstack=10240 -ldatacert -s

# *****************************************
# RESOURCE
# *****************************************
res.o: res.mrc
	morc res.mrc 


